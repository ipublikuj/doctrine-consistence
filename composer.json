{
  "name" : "ipub/doctrine-consistence",
  "type" : "library",
  "description" : "Consistence registration extension for Nette and Doctrine 2",
  "keywords" : [
    "consistence",
    "doctrine",
    "nette",
    "tools",
    "ipub",
    "ipublikuj"
  ],

  "homepage" : "https://github.com/iPublikuj/doctrine-consistence",

  "license" : [
    "GPL-2.0",
    "GPL-3.0"
  ],

  "authors" : [
    {
      "name" : "iPublikuj:cms",
      "email" : "info@ipublikuj.eu",
      "homepage" : "https://www.ipublikuj.eu/"
    }
  ],

  "support" : {
    "email" : "support@ipublikuj.eu",
    "issues" : "https://github.com/iPublikuj/doctrine-consistence/issues"
  },

  "extra" : {
    "ipub" : {
      "configuration" : {
        "extensions" : {
          "doctrineConsistence" : "IPub\\DoctrineConsistence\\DI\\DoctrineConsistenceExtension"
        }
      }
    }
  },

  "require" : {
    "doctrine/orm" : "~2.5",

    "consistence/consistence-doctrine" : "~2.0",

    "nette/di" : "~2.4 || ~3.0"
  },

  "require-dev" : {
    "nette/bootstrap" : "~2.4 || ~3.0",
    "nette/http" : "~2.4 || ~3.0",
    "nette/mail" : "~2.4 || ~3.0",
    "nette/robot-loader" : "~2.4 || ~3.0",
    "nette/safe-stream" : "~2.3",
    "nette/tester" : "~2.3",

    "tracy/tracy" : "~2.4",

    "pds/skeleton" : "~1.0",

    "nettrine/orm" : "~0.3"
  },

  "autoload" : {
    "psr-0" : {
      "IPub\\DoctrineConsistence\\" : "src/"
    }
  }
}
